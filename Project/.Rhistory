xlim(0,20)+
ylim(0,1) +
labs(x = "Nominal Rate (%)", y = "Foreign Investors (%)", title = "Government Debt Held by Foreign Investors (%) X Nominal Rate", subtitle = "45 countries (2004-2019)" ) +
theme_bw()+
